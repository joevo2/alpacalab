<!-- Payment panel -->
<div class="uk-width-medium-1-1">
	<div class="uk-panel">
		<form class="uk-form uk-form-stacked" method="post">
			<fieldset data-uk-margin>
				<legend>Get your pusheen now ðŸ˜„</legend>
				<p class="uk-form-controls-condensed">Pusheen color combination</p>
				<div class="uk-button uk-form-select" data-uk-form-select>
					<span></span>
					<i class="uk-icon-caret-down"></i>
					<select>
						<option>Grey</option>
						<option>White</option>
