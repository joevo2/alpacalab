						<option>Grey</option>
						<option>White</option>
						<option>Black</option>
						<option>Pink</option>
						<option>Orange</option>
					</select>
				</div>
				<input type="email" placeholder="Name" name="name" class="uk-width-1-1">
				<input type="email" placeholder="Email" name="email" class="uk-width-1-1">
				<input type="text" placeholder="Phone Number" name="phone" class="uk-width-1-1">
				<textarea cols="" rows="" placeholder="Address" class="uk-width-1-1" name="address"></textarea>
			</fieldset>
			<div class="uk-margin">
